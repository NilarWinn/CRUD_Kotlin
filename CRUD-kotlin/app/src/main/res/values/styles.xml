<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--Card View-->
    <style name="CardView.Main" parent="CardView">
        <item name="cardBackgroundColor">?attr/colorPrimary</item>
    </style>

    <style name="CardView.Text.Title" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
    </style>

    <style name="CardView.Text.Body" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="Output.Text.Label" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">?attr/colorPrimary</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="Output.Text.Body" parent="Widget.MaterialComponents.TextView">
        <item name="android:textSize">18sp</item>
    </style>

</resources>